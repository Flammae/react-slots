{
	"compilerOptions": {
		"noEmit": true,
		"target": "ESNext",
		"module": "Node16",
		"lib": ["es2022"],
		"strict": true,
		"esModuleInterop": true,
		"moduleResolution": "Node16",
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUncheckedIndexedAccess": true,
		"jsx": "preserve",
		"resolveJsonModule": true,
		"types": ["node"]
	}
}
